<?xml version="1.0"?> 
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD//EN" 
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
    
<hibernate-mapping> 
    <class 
        name="mappingHibernate.MasterypageSummoner" 
        table="masterypage_summoner">             
        <id 
            name="id" 
            column="summonerId">  
        </id>	
        <property 
            name="pages" 
            column="pages"/>

    </class> 
</hibernate-mapping> 
