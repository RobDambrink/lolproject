<?xml version="1.0"?> 
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD//EN" 
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
    
<hibernate-mapping> 
    <class 
        name="mappingHibernate.MatchHistory" 
        table="match_history">             
        <id 
            name="id" 
            column="id">  
            <generator class="increment"/> 
        </id>	
        <property 
            name="gameId" 
            column="gameId"/>
        <property 
            name="summonerId" 
            column="summonerId"/>  
    </class> 
</hibernate-mapping> 
