<?xml version="1.0"?> 
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD//EN" 
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
    
<hibernate-mapping> 
    <class 
        name="mappingHibernate.Accounts" 
        table="account">             
        <id 
            name="id" 
            column="id">  
            <generator class="increment"/> 
        </id>	
        <property 
            name="name" 
            column="name"/>
        <property 
            name="password" 
            column="password"/>
        <property 
            name="summonerID" 
            column="summonerId"/>
    </class> 
</hibernate-mapping> 
