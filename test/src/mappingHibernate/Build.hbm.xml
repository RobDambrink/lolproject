<?xml version="1.0"?> 
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD//EN" 
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
    
<hibernate-mapping> 
    <class 
        name="mappingHibernate.Build" 
        table="build">             
        <id 
            name="id" 
            column="id">  
            <generator class="increment"/> 
        </id>	
        <property 
            name="name" 
            column="name"/>
        <property 
            name="itemPages" 
            column="itempages"/>
        <property 
            name="accountId" 
            column="accountid"/>
       	<property 
            name="championId" 
            column="championId"/>
    </class> 
</hibernate-mapping> 
