<?xml version="1.0"?> 
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD//EN" 
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
    
<hibernate-mapping> 
    <class 
        name="mappingHibernate.BuildComment" 
        table="build_comment">             
        <id 
            name="buildid" 
            column="buildid">  
        </id>	
        <property 
            name="commentid" 
            column="commentid"/>
    </class> 
</hibernate-mapping> 
